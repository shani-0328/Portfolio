/* This SVG represents a pixel art background */
<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Sky background -->
  <rect width="800" height="400" fill="#5651B5" />
  
  <!-- Ground -->
  <rect y="400" width="800" height="200" fill="#8B4513" />
  
  <!-- Grid lines for retro feel -->
  <g stroke="#000" stroke-opacity="0.1" stroke-width="1">
    <!-- Vertical lines -->
    <line x1="0" y1="0" x2="0" y2="600" />
    <line x1="40" y1="0" x2="40" y2="600" />
    <line x1="80" y1="0" x2="80" y2="600" />
    <line x1="120" y1="0" x2="120" y2="600" />
    <line x1="160" y1="0" x2="160" y2="600" />
    <line x1="200" y1="0" x2="200" y2="600" />
    <line x1="240" y1="0" x2="240" y2="600" />
    <line x1="280" y1="0" x2="280" y2="600" />
    <line x1="320" y1="0" x2="320" y2="600" />
    <line x1="360" y1="0" x2="360" y2="600" />
    <line x1="400" y1="0" x2="400" y2="600" />
    <line x1="440" y1="0" x2="440" y2="600" />
    <line x1="480" y1="0" x2="480" y2="600" />
    <line x1="520" y1="0" x2="520" y2="600" />
    <line x1="560" y1="0" x2="560" y2="600" />
    <line x1="600" y1="0" x2="600" y2="600" />
    <line x1="640" y1="0" x2="640" y2="600" />
    <line x1="680" y1="0" x2="680" y2="600" />
    <line x1="720" y1="0" x2="720" y2="600" />
    <line x1="760" y1="0" x2="760" y2="600" />
    <line x1="800" y1="0" x2="800" y2="600" />
    
    <!-- Horizontal lines -->
    <line x1="0" y1="0" x2="800" y2="0" />
    <line x1="0" y1="40" x2="800" y2="40" />
    <line x1="0" y1="80" x2="800" y2="80" />
    <line x1="0" y1="120" x2="800" y2="120" />
    <line x1="0" y1="160" x2="800" y2="160" />
    <line x1="0" y1="200" x2="800" y2="200" />
    <line x1="0" y1="240" x2="800" y2="240" />
    <line x1="0" y1="280" x2="800" y2="280" />
    <line x1="0" y1="320" x2="800" y2="320" />
    <line x1="0" y1="360" x2="800" y2="360" />
    <line x1="0" y1="400" x2="800" y2="400" />
    <line x1="0" y1="440" x2="800" y2="440" />
    <line x1="0" y1="480" x2="800" y2="480" />
    <line x1="0" y1="520" x2="800" y2="520" />
    <line x1="0" y1="560" x2="800" y2="560" />
    <line x1="0" y1="600" x2="800" y2="600" />
  </g>
  
  <!-- Small clouds (pixel style) -->
  <g fill="#FFFFFF">
    <rect x="100" y="60" width="80" height="20" />
    <rect x="120" y="40" width="40" height="20" />
    
    <rect x="320" y="100" width="100" height="20" />
    <rect x="340" y="80" width="60" height="20" />
    
    <rect x="580" y="70" width="120" height="20" />
    <rect x="600" y="50" width="80" height="20" />
  </g>
  
  <!-- Small mountains (pixel style) -->
  <polygon points="0,400 120,280 240,400" fill="#654321" />
  <polygon points="200,400 320,200 440,400" fill="#654321" />
  <polygon points="400,400 560,180 720,400" fill="#654321" />
  <polygon points="680,400 760,320 800,400" fill="#654321" />
  
  <!-- Trees (pixel style) -->
  <g>
    <rect x="80" y="350" width="16" height="50" fill="#8B4513" />
    <rect x="60" y="330" width="56" height="20" fill="#006400" />
    <rect x="70" y="310" width="36" height="20" fill="#006400" />
    <rect x="80" y="290" width="16" height="20" fill="#006400" />
    
    <rect x="500" y="350" width="16" height="50" fill="#8B4513" />
    <rect x="480" y="330" width="56" height="20" fill="#006400" />
    <rect x="490" y="310" width="36" height="20" fill="#006400" />
    <rect x="500" y="290" width="16" height="20" fill="#006400" />
    
    <rect x="650" y="370" width="16" height="30" fill="#8B4513" />
    <rect x="630" y="350" width="56" height="20" fill="#006400" />
    <rect x="640" y="330" width="36" height="20" fill="#006400" />
    <rect x="650" y="310" width="16" height="20" fill="#006400" />
  </g>
  
  <!-- Grass tufts -->
  <g fill="#007700">
    <rect x="20" y="400" width="8" height="8" />
    <rect x="60" y="400" width="8" height="8" />
    <rect x="140" y="400" width="8" height="8" />
    <rect x="210" y="400" width="8" height="8" />
    <rect x="272" y="400" width="8" height="8" />
    <rect x="340" y="400" width="8" height="8" />
    <rect x="410" y="400" width="8" height="8" />
    <rect x="455" y="400" width="8" height="8" />
    <rect x="530" y="400" width="8" height="8" />
    <rect x="590" y="400" width="8" height="8" />
    <rect x="670" y="400" width="8" height="8" />
    <rect x="730" y="400" width="8" height="8" />
    <rect x="780" y="400" width="8" height="8" />
  </g>
</svg>
